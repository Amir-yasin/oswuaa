{
    "version": 2,
    "builds": [
        {
            "src": "osu_project/osu_project/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.11.4" }
        },
        {
            "src": "manage.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "osu_project/osu_project/wsgi.py"
        }
    ]
}
